  <div class="accordionElement">
    <div class="accordionTitle">
      <div class="img" style="background-image: url('{{Cover}}')"></div><span>{{Name}}</span>
    </div>
    <div class="accordionContent">

      <div class="line indent">
        {{#if futureEvent}}{{{Description}}}{{else}}{{{End-Description}}}{{/if}}
        <BR><BR>

          <!-- {{#if "Photos"}} -->
          {{#each "Photos"}}
          <img src="{{this}}">
          {{/each}}
          <!-- {{else}} -->
          <img src="{{Cover}}">
          <!-- {{/if}} -->



        <div class="clear"></div>
      </div>

      {{#if futureEvent}}<div class="line indent">
        Cost: Free {{#if Donation}}(Suggested donation: ${{Donation}}){{/if}}
      </div>
      {{/if}}

      {{#if futureEvent}}

        {{#if RSVP}}
        <div class="line indent">
          RSVP [<a href="{{RSVP}}" target="_blank">{{RSVP}}</a>]
        </div>
        {{/if}}
      {{else}}
      {{/if}}

      {{#if TeacherNames}}
      <div class="line indent">
        Teacher: {{#each TeacherNames}}{{this}}{{#unless @last}},{{/unless}} {{/each}}
      </div>
      {{/if}}

      {{#if LabTechNames}}
      <div class="line indent">
        Lab Technician: {{#each LabTechNames}}{{this}}{{#unless @last}},{{/unless}} {{/each}}
      </div>
