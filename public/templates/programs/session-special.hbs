<body>
  {{> header}}
  {{> footer}}
  <div class="crt-overlay crt-overlay-fullsize"></div>

  <div id="shell" class="shell shell-fullsize about {{"Session Slug"}} sfpc">

    <div class="loader-content">
      <p tabindex="0" class="large">
        loading<span>...</span>
      </p>
    </div>

    <div class="selectors {{#if registrationDone}}regDone{{/if}}">
      <select id="allSections" onchange="sectionJump(this.value)" class="{{"Session Slug"}} hide-content">
        <option value="class">Jump to section...</option>
        <option value="section-class-details">ABOUT </option>
        <option value="section-class-details">--Details & Description</option>
        {{#if showcase}}<option value="section-showcase">--Outcomes </option>{{else}}{{/if}}
        <option value="section-course-of-study">--Course of Study</option>
        <option value="section-expectations">--Expectations</option>
        <option value="section-is-this-class-for-me">--Is this for me?</option>
        <option value="section-teachers">--Teachers{{#if guests}} & Guests{{/if}}</option>
        <option value="section-accessibility" class="hide-regDone">--Accessibility</option>
        <option value="section-apply" class="hide-regDone">APPLY NOW</option>
        <option value="section-cost" class="hide-regDone">FINANCES</option>
        <option value="section-cost" class="hide-regDone">--Cost of Attendence</option>
        <option value="section-scholarships" class="hide-regDone">--Scholarships</option>
        <option value="section-donate" class="hide-regDone">--Donate to SFPC</option>
        <option value="section-refund" class="hide-regDone">--Refund Policy</option>
        <option value="section-classes">OPPORTUNITIES</option>
      </select>
    </div>





    <h4 class="breadcrumb hide-content">
      <a href="/participate.html">participate</a>
      <span class="caret"></span>
      <a href="">sessions</a>
      <!-- <span class="caret"></span>
      <a href="/sessions/{{'Session Slug'}} " id=" session-link">{{"Session Name"}}</a> -->
    </h4>




    <div id="class"
      class=" hide-content {{#if applicationsOpen}}appsOpen{{else}}appsClosed{{/if}} {{#if registrationDone}}regDone{{/if}} {{#if sessionEnded}}sessionEnded{{/if}} shell-content">


      <a class="fixed-apply hide-apply hide-appsClosed" href="{{applicationLink}}">Apply Now</a>


      <section id="section-class-details">
        <div class="grid-2">
          <h1>
            <span id="session-name">{{name}}</span>
           </h1>
        </div>

        <div class="grid-2 class-details">



          <img id="class-image" src="{{[Banner Image].[0]}}">
        </div>
      </section>

      <div class="break-noline"></div>

      <section class="intro {{Website-Slug}}" id="introduction">
        <p tabindex="0" class="large">
          <span id="introduction-text" style="white-space:pre-line">{{#if sessionEnded}}{{#if "Session End Description"}}{{"Session End Description"}}{{else}}{{Description}}{{/if}}{{else}}{{Description}}{{/if}}</span><BR><BR>
          <span id="session-name">{{"Session Name"}}</span> {{#if sessionEnded}}was{{else}}is{{/if}}
          organized by
          {{#if organizerTeachers}}
              {{#each organizerTeachers}}
              {{#if @last}}{{#if @first}}{{else}} and{{/if}}{{/if}}
              <span class="bio" style="background-image: url('{{this.Image.[0]}}')" role="img"> </span>

              <span tabindex="0" class="expand"><a class="link">{{this.Name}}</a><span class="expanded hide-expanded">{{this.Bio}}</span></span>{{/each}}{{/if}}{{#if justOrganizers}}{{#if organizerTeachers}} with {{/if}}
              {{#each justOrganizers}}{{#if @last}}{{#if @first}}{{else}} and{{/if}}{{/if}}
              <span class="bio" style="background-image: url('{{this.Image.[0]}}')" role="img"> </span> <span tabindex="0" class="expand"><a class="link">{{this.Name}}</a><span class="expanded hide-expanded">{{this.Bio}}</span></span>{{/each}}{{/if}}.


          It include{{#if sessionEnded}}d{{else}}s{{/if}} classes and workshops taught by
          {{#each teachers}}
          {{#if @last}}{{#if @first}}{{else}} and{{/if}}{{/if}}
          <span class="bio" style="background-image: url('{{this.Image.[0]}}')" role="img"></span>

          <span tabindex="0" class="expand"><a class="link">{{this.Name}}</a><span class="expanded hide-expanded">{{this.Bio}}</span></span>{{/each}}

          {{#if guests}}and guests
          {{#each guests}}
          {{#if @last}}{{#if @first}}{{else}}and{{/if}}{{/if}}
          <span class="bio" style="background-image: url('{{this.Image.[0]}}')" role="img"></span>
          <span tabindex="0" class="expand"><a class="link">{{this.Name}}</a><span class="expanded hide-expanded">
          {{this.Bio}}</span></span>{{/each}}{{else}}{{/if}}.


          <!-- <BR><BR> -->
          <!-- Applications launching soon. <a href="">Get Notified</a>. -->

          <!-- <a href="{{applicationLink}}" class="hide-appsClosed">Apply Now</a> -->
        </p>
      </section>

      <div class="break-noline"></div>

      <section id="section-class-details">
        <div class="grid-2">
          <h1><span id="class-name">
              <!-- {{name}} -->
              Details
            </span></h1>
        </div>

        <div class="grid-2 class-details">



          <!-- <img id="class-image" src="{{promoImage}}"> -->

          <!-- <div class="details">
            <div class="labels">Teachers</div>
            <div class="labels-info" id="teachers-list">{{#each teachers}}{{this.Name}}{{#unless
              @last}}, {{/unless}}{{/each}}
            </div>
          </div> -->
          <div class="details">
            <div class="labels">Date </div>
            <div class="labels-info">
              <span id="date">{{startDate}} to {{endDate}}</span>{{#if numberOfClasses}} (<span id="number-classes">{{numberOfClasses}}</span> classes){{/if}}

            </div>
          </div>
          <div class="details">
            <div class="labels">Time </div>
            <div class="labels-info" id="time">{{time}}
            </div>
          </div>
          <div class="details">
            <div class="labels">Location </div>
            <div class="labels-info" id="location">{{location}}</div>
          </div>
          <div class="details">
            <div class="labels">Cost </div>
            <div class="labels-info"><span id="cost">${{cost}}</span> Scholarships available
              {{#if registrationDone}}<a href="/about/scholarships">{{else}}<a class="more-about-cost">{{/if}}

                learn more...</a></div>
          </div>
          <div class="details">
            <div class="labels">Deadline </div>
            <div class="labels-info">
              <span class="">Applications {{#if applicationsOpen}}open
                until {{else}}closed on {{/if}}</span>
              <span id="deadline">{{applicationEndDate}}</span>
            </div>
          </div>

          <p class="large apply hide-appsClosed">
            <a href="{{applicationLink}}" target="_blank" id="application-link">Apply Now</a>
          </p>


        </div>
      </section>

      <!-- <div class="break"></div> -->


      <!-- <section id="section-description">
        <div class="grid-2">
          <h1>Description </h1>
        </div>

        <div class="grid-2">
          <p style="white-space:pre-line">
            {{trim description}}
          </p>
        </div>
      </section> -->


            <div class="break-noline"></div>

      {{#if sessionEnded}}

        {{#if sessionImages.[1]}}
            <div class="grid-container" id="participate-archive">

              <main class="grid-item main">
                <main class="grid-item main">
                  <div class="carousel-items items carousel-content participate-content">
                    <div id="carousel-images" class="class-images after-launch">

                      {{#each sessionImages}}
                      <figure class="item participate-img"><img src="{{this}}">
                      </figure>
                      {{/each}}
                      {{#if promoGraphic}}
                      <figure class="item participate-img"><img src="{{promoGraphic}}">
                      </figure>
                      {{/if}}

                    </div>


                  </div>


                </main>
              </main>

            </div>



            <section class="section-testimonials">
                 <div class="grid-2">
                   <p>
                     <span  class="text-on-bg">
                     <i>Images courtesy of teachers{{#if guests}}, participants, class documentarians and guests{{else}}, participants and class documentarians{{/if}}.</i>
                   </span>
                   </p>
                 </div>

                 <div class="grid-2">
                 </div>
               </section>


               {{else}}

               {{#if promoImages.[1]}}
                   <div class="grid-container" id="participate-archive">

                     <main class="grid-item main">
                       <main class="grid-item main">
                         <div class="carousel-items items carousel-content participate-content">
                           <div id="carousel-images" class="class-images after-launch">

                             {{#each promoImages}}
                             <figure class="item participate-img"><img src="{{this}}">
                             </figure>
                             {{/each}}
                             {{#if promoGraphic}}
                             <figure class="item participate-img"><img src="{{promoGraphic}}">
                             </figure>
                             {{/if}}
                           </div>


                         </div>


                       </main>
                     </main>

                   </div>



                   <section class="section-testimonials">
                        <div class="grid-2">
                          <p>
                            <span  class="text-on-bg">
                            <i>Images courtesy of teachers{{#if guests}} and guests{{else}}{{/if}}.</i>
                          </span>
                          </p>
                        </div>

                        <div class="grid-2">
                        </div>
                      </section>
                   {{/if}}



          {{/if}}


      {{else}}



          {{#if promoImages.[1]}}
              <div class="grid-container" id="participate-archive">

                <main class="grid-item main">
                  <main class="grid-item main">
                    <div class="carousel-items items carousel-content participate-content">
                      <div id="carousel-images" class="class-images after-launch">

                        {{#each promoImages}}
                        <figure class="item participate-img"><img src="{{this}}">
                        </figure>
                        {{/each}}
                        {{#if promoGraphic}}
                        <figure class="item participate-img"><img src="{{promoGraphic}}">
                        </figure>
                        {{/if}}
                      </div>


                    </div>


                  </main>
                </main>

              </div>



              <section class="section-testimonials">
                   <div class="grid-2">
                     <p>
                       <span  class="text-on-bg">
                       <i>Images courtesy of teachers{{#if guests}} and guests{{else}}{{/if}}.</i>
                     </span>
                     </p>
                   </div>

                   <div class="grid-2">
                   </div>
                 </section>


            {{/if}}




      {{/if}}





      {{#if showcase}}

       <div class="break"></div>
       <section id="section-showcase">
         <div class="grid-2">
           <h1><span id="class-name">
               Outcomes
             </span></h1>


         </div>
         <div class="grid-2 showcase-list class-details">
           {{#if classEvents}}
           <h6>Events ({{eventCount}})</h6>
            {{#each classEvents}}
            <div class="grid-1 featured showcase">
              <div class="featured-text">
                <div class="left">
                  <h4 class="type">
                    <a href="/events/{{this.slug}}">{{this.date.start}}</a>
                  </h4>
                    <a href="/events/{{this.slug}}" target="_blank"><h2 class="title">{{this.name}} </h2></a>
                </div>
                <span class="bio bio-float" style="background-image: url('{{this.image}}')" role="img"></span>
              </div>
            </div>
            {{/each}} <BR>
            {{/if}}

            {{#if classProjects}}
            <h6>Projects ({{projectCount}})</h6>
             {{#each classProjects}}
             <div class="grid-1 featured showcase">
               <div class="featured-text">
                 <div class="left">
                   <h4 class="type">
                     <a href="/projects/{{this.slug}}">{{this.date.start}}</a>
                   </h4>
                     <a href="/projects/{{this.slug}}" target="_blank"><h2 class="title">{{this.name}} </h2></a>
                 </div>
                 <span class="bio bio-float" style="background-image: url('{{this.image.[0]}}')" role="img"></span>
               </div>
             </div>
             {{/each}} <BR>
             {{/if}}

             {{#if classBlog}}
             <h6>Blog Posts ({{blogCount}})</h6>
             {{#each classBlog}}
             {{#eq this.status.[0] "Published"}}
             <div class="grid-1 featured showcase">
               <div class="featured-text">
                 <div class="top">
                   <h4 class="type">
                     <a href="/blog/{{this.slug}}">{{this.date.start}}</a>
                   </h4>
                     <a href="/blog/{{this.slug}}" target="_blank"><h2 class="title">{{this.name}} </h2></a>
                 </div>

                 <span class="bio bio-float" style="background-image: url('{{this.image.[0]}}')" role="img"></span>
               </div>
             </div>
             {{/eq}}
             {{/each}} <BR>
             {{/if}}



              </div>

             </section>

             {{/if}}







      {{#each pageContent}}


            <div class="break"></div>


      <section id="section-{{dashcase @key}}" class="program-content">
        <div class="grid-2">
          <h1>{{@key}}</h1>
        </div>

        <div class="grid-2">
          {{{this}}}
        </div>
      </section>


      {{/each}}

      <div class="break"></div>


      <section id="section-teachers">
        <div class="grid-2">
          <h1>Meet the {{#if organizers}}Organizers{{#if teachers}}{{#if guests}},{{else}} & {{/if}}{{/if}}{{/if}} Teachers {{#if guests}}& Guests{{/if}}</h1>
        </div>

        <div class="grid-2 class-details">


          <dl class="accordion">
            {{#each organizerTeachers}}
            {{>personCard person=this}}
            {{/each}}

            {{#each justOrganizers}}
            {{>personCard person=this}}
            {{/each}}

            {{#if guests}}

              {{#each guests}}
              {{>personCard person=this}}
              {{/each}}

              {{/if}}

          </dl>


        </div>

      </section>



      <div class="break"></div>


      {{> classBoilerplate}}




    </div>










    <script src="/outside.js"></script>
    <script src="/carousel.js"></script>
    <script src="/closed.js"></script>
    </p>

</body>

</html>
