                <span class="person-group-small questioner">
                {{#if People}} · <a href="/sex-ed-people">
                  {{#each this.[People]}}
                  <span class="{{this}} person-small"></span>
                  {{/each}} </a>
                  {{/if}}
                </span>

            </div>
            <div class="topic"> {{#each this.[Topic]}} {{this}} {{/each}}
            </div>
          </div>

