    src="https://www.statcounter.com/counter/counter.js" async></script>
    <noscript><div class="statcounter"><a title="website statistics"
    href="https://statcounter.com/" target="_blank"><img class="statcounter"
    src="https://c.statcounter.com/12714909/0/6086a46c/1/" alt="website statistics"
    referrerPolicy="no-referrer-when-downgrade"></a></div></noscript>


    <!-- <script data-goatcounter="https://sfpc.goatcounter.com/count"
    async src="//gc.zgo.at/count.js"></script> -->
    <!-- end stats -->
</head>


<body>
{{> header}}
{{> footer}}

      <div class="crt-overlay crt-overlay-fullsize"></div>

      <div id="shell" class="shell shell-fullsize about sfpc">

        <div class="loader-content">
            <p tabindex="0" class="large">
              loading<span>...</span>
            </p>
        </div>

        <!-- <div class="selectors">
        <select id="allSections" onchange="sectionJump(this.value)" class=" hide-content">
              <option value="class">Jump to section...</option>
              <option value="section-class-details">ABOUT </option>
            </select>
          </div>-->





            <h4 class="breadcrumb hide-content">
              <a href="/projects">projects</a>
              <!-- <span class="caret"></span>  -->
            </h4>




        <div id="class" class=" hide-content appsClosed beforeClass shell-content">


          <a class="fixed-apply hide-apply hide-appsClosed" href="">Apply Now</a>

                <section id="section-class-details">
                    <div class="grid-2">
                      <h1><span id="class-name">
                        {{Name}}
                      </span></h1>
                    </div>

                    <div class="grid-2 class-details">


                      <h4 class="noline">
                      {{Status.[0]}} {{[Publish Date]}}

                      <a href="/sessions/{{[Session-Slug]}}"> <span class="blog-tag"><span id="tag" class="{{[Session-Slug]}}">{{[Website-Session]}}</span></span></a>



                      </h4>

                      <div class="break-noline-small"></div>


                      <img id="class-image" src="{{[Image URL]}}">

                      <div class="details">
                        <div class="labels">Type</div>
                        <div class="labels-info" id="teachers-list">{{#each [Product Type]}}{{this}}{{#unless @last}},{{/unless}} {{/each}}</div>
                      </div>
                      <div class="details">
                        <!-- <div class="labels">Status</div>
                        <div class="labels-info" id="teachers-list">{{Status.[0]}} {{[Release Date].start}}</div> -->
                      </div>



                      {{#if Website-Organizers}}
                      <div class="details">
                        <div class="labels">Organizers</div>
                        <div class="labels-info" id="teachers-list">{{#each Website-Organizers}}{{this}}{{#unless @last}},{{/unless}} {{/each}} {{#each this.[Additional organizers]}} {{this}} {{/each}}</div>
                      </div>
                      {{/if}}

                      {{#if Website-Contributors}}
                      <div class="details">
                        <div class="labels">Contributors</div>
                        <div class="labels-info">
                          <span tabindex="0" class="expand"><a class="link">{{Website-Contributors.length}} contributors...</a>
                          <span class="expanded hide-expanded">
                          {{#each Website-Contributors}}{{this}}{{#unless @last}},{{/unless}} {{/each}}
                        </span></span>
                        </div>
                      </div>
                      {{/if}}

                      {{#if Partnership?}}
                      <div class="details">
                        <div class="labels">Partners</div>
                        <div class="labels-info" id="teachers-list">{{Partnership?}} {{Partners}}</div>
                      </div>
                      {{/if}}


                                              {{#if Website-Session}}
                                              <div class="details">
                                                <div class="labels">Program</div>
                                                <div class="labels-info" id="program-info">

                                                  {{#if Website-Class}}
                                                  {{#each Website-Class}}{{this}}{{#unless @last}}, {{/unless}} {{/each}} {{/if}}

                                                  <!-- <span class="Session {{Class-Type}}">
                                                  {{#if Website-Class}}— {{/if}}

                                                  {{#each Website-Session}}
                                                  {{this}}
                                                  {{#unless @last}}, {{/unless}}{{/each}}

                                                  </span> -->

                                                  <!-- {{#if Website-Session}}
                                                  <h4 class="noline">
                                                  {{#if Website-Session}}<a href="{{this.[Session-External-Website]}}">{{else}}<a href="{{[Session-Slug]}}">{{/if}}
                                                      <span class="blog-tag"> <span id="tag" class="{{[Session-Slug]}}">{{[Website-Session]}} </span> </span></a>
                                                    </h4>
                                                        {{/if}} -->


                                                </div>
                                              </div>
                                              {{/if}}

                                              {{#if Price}}

                                              <div class="details">
                                                <div class="labels">Cost</div>
                                                <div class="labels-info" id="">
                                                  <h3>
                                                <span id=”cost”>${{Price}}</span>
                                                 {{{Cost-Details}}}</h3>
                                                </div>
                                              </div>
                                              <div id="add-to-cart-button-wrapper"/>
                                                  {{/if}}


                      <p class="large apply">
                        {{#if Website-URL}}
                        <a href="{{"Website-URL"}}" target="_blank" id="application-link">
                        {{/if}}

                          {{"Website URL CTA"}}

                        {{#if Website-URL}}
                        </a>
                        {{/if}}
                      </p>


                      <!-- {{#if Cost}}
                        <h3>
                      <span id=”cost”>${{Cost}}</span>
                       {{{Cost-Details}}}</h3>
                      {{/if}} -->

                      {{#if More-Info}}
                      <p>
                     {{{More-Info}}}</p>
                      {{/if}}

                    </div>
                </section>


                {{#if [Product Description]}}
                <div class="break"></div>
                <section id="section-description">
                    <div class="grid-2">
                      <h1>Description </h1>
                    </div>
                    <div class="grid-2">
                      <p>
                        {{{[Product Description]}}}
                        </p>
                      <!-- <p class="large apply">
                        <a href="{{websiteURL}}" target="_blank" id="application-link">Visit Website</a>
                      </p> -->
                    </div>
                </section>
                {{/if}}




                {{#if Images}}

                <div class="break-noline"></div>

               <div class="grid-container" id="participate-archive">

                 <main class="grid-item main">
                   <main class="grid-item main">
                     <div class="carousel-items items carousel-content participate-content">


                       <div id="carousel-images" class="class-images ">

                         {{#each Images}}
                         <figure class="item participate-img"><img src="{{this}}"></figure>
                         {{/each}}

                         </div>


                     </div>


                   </main>
                 </main>

               </div>
               {{/if}}


               <div class="break"></div>

               <div class="break-noline"></div>


               <section id="section-outro">
                 <p tabindex="0" class="large">
                   Interested in working with us on a special project? Get in touch over <a href="mailto:info@sfpc.study">email</a>.
                   Support our programming and future projects through <a href="https://withfriends.co/school_for_poetic_computation/join">funding</a>.
                   Visit our <a href="https://sfpc.study/projects">project archive</a> to see more projects.
                 </p>
               </section>



                <div class="break-endpage"></div>








        </div>

<script src="/outside.js"></script>
<script src="/carousel.js"></script>
<!-- <script src="/closed.js"></script> -->
</p>

</body>
</html>
