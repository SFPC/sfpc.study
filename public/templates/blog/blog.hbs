          <div class="break-noline"></div>

          <section id="section-description">

              <div class="stacked blog-list">

                {{#each blog}}

                {{#eq status "Published"}}

                  <div id="{{this.[Website-Slug]}}"
                  class="grid-1 table blog-list-post {{#each this.[Project-Type]}}{{this}} {{/each}}" data-location="">
                  <div class="featured-text">

                    <div class="left">
                      <h4 class="">
                        {{[Publish-Date].start}}
                        {{#if this.[Session-Slug]}}

                        <a href="/sessions/{{[Session-Slug]}}">


                            <span class="blog-tag"><span id="tag" class="{{[Session-Slug]}}">

                              {{[Website-Session]}}


                            </span></span></a>
                              {{/if}}
                      </h4> <BR>
                      <h2 class="title">
                      <a href="/blog/{{this.[Website-Slug]}}">  {{this.Name}}</h2>  </a>

                      {{#if this.[Website-Authors]}}

                      <h6>
                        <!-- {{this.[Short Description]}}... -->
                        written by {{#each this.[Website-Authors]}}{{this}}{{#if @last}}{{else}}<i class="comma">,</i> {{/if}} {{/each}}

