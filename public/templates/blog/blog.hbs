                      </h6>
                      {{/if}}
                    </div>

                    {{#if Cover-Image}}
                    <div class="right">
                      <img src="{{this.[Cover-Image].[0]}}">
                    </div>
                    {{/if}}

                  </div>
                </div>


                {{else}}
                {{/eq}}

                {{/each}}

              </div>
            </section>


